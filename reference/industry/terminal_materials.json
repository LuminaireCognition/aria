  "default_terminal": {
    "description": "Materials that are typically bought rather than built, but CAN be manufactured",
    "pi_p2_plus": {
      "terminal_by_default": true,
      "note": "P2, P3, P4 can be manufactured at customs offices but are usually bought",
      "items": "See material_sources.json pi_p2, pi_p3, pi_p4 arrays"
