  "chain_depth_limits": {
    "description": "Safety limits to prevent infinite recursion",
    "max_depth": 5,
    "rationale": "T1 ships rarely go deeper than 2-3 levels. T2/T3 can go deeper but we cap at 5."
  },

  "resolution_modes": {
    "buy_all_components": {
      "description": "Stop at any manufactured component - show market prices only",
      "terminals": ["all materials that have blueprints"]
    },
    "build_ship_components": {
      "description": "Build ship components (Seals, Regulators, etc.) but buy PI/reactions",
      "build": ["ship_components"],
      "terminal": ["pi_p2", "pi_p3", "pi_p4", "reaction_intermediates"]
    },
    "build_all": {
      "description": "Build everything possible down to raw materials",
      "terminal": ["always_terminal items only"]
    }
